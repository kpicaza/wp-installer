{
    "name": "kpicaza/wp-installer",
    "description": "Package to build a wordpress site",
    "authors": [
        {
            "name": "kpicaza"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress/wordpress",
                "type": "wordpress-core",
                "version": "3.5.1",
                "source": {
                    "type": "git",
                    "url": "https://github.com/WordPress/WordPress.git",
                    "reference": "3.5.1"
                },
                "require": {
                    "johnpbloch/wordpress-core-installer": "~0.1"
                }
            }
        }
    ],
    "require": {
        "wordpress/wordpress": "3.5.*"
    },
    "extra": {
        "wordpress-install-dir": {
            "wordpress/wordpress": "wp",
            "johnpbloch/wordpress": "jpb-wordpress"
        }
    }
}
